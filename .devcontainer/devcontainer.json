{
    "name": "devenv",
    "dockerComposeFile": "compose.yml",
    "service": "dev-env",
    "workspaceFolder": "/workspace",
    "remoteUser": "ubuntu",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "oderwat.indent-rainbow",
          "charliermarsh.ruff",
          "tamasfe.even-better-toml",
          "humao.rest-client",
          "qwtel.sqlite-viewer",
          "formulahendry.code-runner",
          "mhutchie.git-graph",
          "ms-python.isort",
          "Gruntfuggly.todo-tree"
        ]
      }
    },
    "postCreateCommand": "make"
  }
